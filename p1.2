#include <iostream>
using namespace std;
bool prime(long long int n){
	if( n<=1)return 0;
	for(long long int i=2;i*i<=n;i++){              // Partially Accepted
		if(n%i==0) return 0;
	}
	return 1;
}
bool check(long long int n){
	if(prime(n+1)){
		return 0;
	}
	else return 1;
}
int main() {
	int t;
	cin>>t;
	while(t--){
		long long int x;
		cin>>x;
		if(check(x)) cout<<"YES\n";
		else cout<<"NO\n";
	}
}
