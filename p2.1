void perm(string s, int ind,set<string> &v){
	   if(ind>=s.length()){
	       v.insert(s);
	         return;    
	   }
	   
	   for(int i=ind;i<s.length();i++){
	        swap(s[ind],s[i]); 
	        perm(s,ind+1,v);
	        swap(s[i],s[ind]);
	       
	   }
	    
	}
		vector<string>find_permutation(string S)
		{
		    set<string> v;
		    perm(S,0,v);
		    vector<string>res;
		    res.assign(v.begin(),v.end());
		    sort(res.begin(),res.end());
		    return res;
		}
