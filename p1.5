#include<bits/stdc++.h>
using namespace std;

bool prime(int n){
	if(n<=1) return 0;
	for(int i =2; i<=sqrt(n);i++){                	// Partially Accepted
		if(n%i==0) return 0;
	}
	return 1;
}
int sof(int n){
	bool prime[n + 1];
	int res = 0;
    memset(prime, true, sizeof(prime));
    for (int p = 2; p * p <= n; p++) {
		if (prime[p] == true) {
            for (int i = p * p; i <= n; i += p)
                prime[i] = false;
        }
	}
   	for (int p = 2; p <= n; p++)
        if (prime[p])
			res++;
	return res;
}
int check(int l=3, int r=10){
	int result=0;
	for(int x = l;x<=r ;x++){
		if(prime(sof(x))){
			result++;
		}
	}
	return result;
}

int main(){
	int t ;
	cin>>t;
	while(t--){
		int l,r;
		cin>>l>>r;
		cout<<check(l,r)<<endl;
	}
}
