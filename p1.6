#include<bits/stdc++.h>
using namespace std;
bool prime(int n){
	if(n<=1) return 0;
	for(int i=2;i*i<=n;i++){
		if(n%i==0) return 0;
	}
	return 1;
}
string check(string s){
	unordered_map<char,int> mp;
	for(int i=0;i<s.length();i++){
		mp[s[i]]++;
	}
	bool dist = prime(mp.size());
	bool occ = true;;
	for(auto x: mp){
		if(prime(x.second)==0){
			occ = false;
			break;
		}
	}
	if(dist==1 && occ==1) return "YES";
	return "NO";
}
int main(){
	int t;
	cin>>t;
	while(t--){
		string s;
		cin>>s;
		cout<<check(s)<<endl;
	}
	return 0;
}
